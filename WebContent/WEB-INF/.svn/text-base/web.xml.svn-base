<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>nmdss</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<listener>
		<listener-class>cn.com.st.dss.utils.HsqldbListner</listener-class>
	</listener>
	<servlet>
		<description></description>
		<display-name>Page</display-name>
		<servlet-name>Page</servlet-name>
		<servlet-class>cn.com.si.dss.gd.nm.web.IndexPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Page</servlet-name>
		<url-pattern>/index.php</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DisplayChart</servlet-name>
		<servlet-class>org.jfree.chart.servlet.DisplayChart</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisplayChart</servlet-name>
		<url-pattern>/servlet/DisplayChart</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>queryController</display-name>
		<servlet-name>queryController</servlet-name>
		<servlet-class>cn.com.st.dss.catv.controller.queryController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>queryController</servlet-name>
		<url-pattern>/queryController</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>TableController</display-name>
		<servlet-name>TableController</servlet-name>
		<servlet-class>cn.com.si.dss.controller.TableController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TableController</servlet-name>
		<url-pattern>/tableController</url-pattern>
	</servlet-mapping>

	<servlet>
		<description></description>
		<display-name>MyChart</display-name>
		<servlet-name>MyChart</servlet-name>
		<servlet-class>cn.com.st.dss.utils.jfreechart.web.MyChart</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyChart</servlet-name>
		<url-pattern>/myChart</url-pattern>
	</servlet-mapping>
<servlet>
	<servlet-name>testchart</servlet-name>
	<servlet-class>cn.com.st.dss.utils.jfreechart.Test</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>testchart</servlet-name>
	<url-pattern>/testChart</url-pattern>
</servlet-mapping>
	<servlet>
		<servlet-name>web-action</servlet-name>
		<servlet-class>com.sitech.nmdss.mvc.action.ActionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>web-action</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
</web-app>