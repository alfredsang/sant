<?xml version="1.0" encoding="UTF-8"?>
<flexigrid>
	<gridName>grid</gridName>
	<width>100</width>
	<height>200</height>
	<querySql>"(select * from si_dss_virtual_workspace)"</querySql>
	<url>'/nmdss/queryController?action=list'</url>
	<dataType>'json'|'xml'</dataType>
	<colModel>
		<colModelItem>
			   <display>'编号'</display>
				<name>'id'</name>
				<width>50</width>
				<sortable>true|false</sortable>
				<align>'left'|'center'|'right'</align>
				<toggle>true|false</toggle>
		</colModelItem>
	</colModel>
	<searchitems>
		<searchitem>
			<display>'编号'</display>
			<name>'id'</name>
			<isdefault>true|false</isdefault>		
		</searchitem>
	</searchitems>
	<errormsg>'发生异常'</errormsg>
	<sortname>"id"</sortname>
	<sortorder>"desc"|"asc"</sortorder>
	<qop>""|"LIKE"</qop>
	<usepager>true</usepager>
	<title> '测试信息明细'</title>
	<pagestat>'显示记录从{from}到{to}，总数 {total} 条'</pagestat>
	<useRp>true|false</useRp>
	<rp>10</rp>
	<rpOptions> [ 10, 15, 20, 30, 40, 100 ]</rpOptions>
	<nomsg>'没有符合条件的记录存在'</nomsg> 
	<minColToggle>1</minColToggle>
	<showTableToggleBtn>true|false</showTableToggleBtn>
	<autoload>true|false</autoload>
	<resizable>false</resizable> 
	<procmsg>'加载中, 请稍等 ...'</procmsg>
	<hideOnSubmit>true|false</hideOnSubmit>
	<blockOpacity>0.5</blockOpacity>
	<showcheckbox>true|false</showcheckbox>
	<gridClass>"bbit-grid"</gridClass> 
	<rowhandler>contextmenu</rowhandler>
	<rowbinddata>true</rowbinddata>
	<onrowchecked>false|true|function</onrowchecked>
</flexigrid>